{
    "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
    "release-type": "node",
    "include-component-in-tag": false,
    "changelog-path": "CHANGELOG.md",
    "packages": {
        "src/elements": {
            "component": "lyne-elements",
            "changelog-path": "../../CHANGELOG.md"
        },
        "src/elements-experimental": {
            "component": "lyne-elements-experimental",
            "changelog-path": "../../CHANGELOG.md"
        },
        "src/angular": {
            "component": "lyne-angular",
            "include-component-in-tag": true,
            "changelog-path": "../../CHANGELOG.md"
        },
        "src/angular-experimental": {
            "component": "lyne-angular-experimental",
            "include-component-in-tag": true,
            "changelog-path": "../../CHANGELOG.md"
        }
    },
    "plugins": [
        {
            "type": "linked-versions",
            "groupName": "core",
            "components": [
                "lyne-elements",
                "lyne-elements-experimental"
            ]
        },
        {
            "type": "linked-versions",
            "groupName": "angular",
            "components": [
                "lyne-angular",
                "lyne-angular-experimental"
            ]
        }
    ]
}